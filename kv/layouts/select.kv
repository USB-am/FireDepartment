<FDSelectListElement>:
	orientation: 'horizontal'
	size_hint_y: None
	height: dp(50)

	MDCheckbox:
		id: checkbox
		size_hint: (None, None)
		size: (dp(50), dp(50))

	MDTextButton:
		id: info_btn
		size_hint: (1, 1)
		theme_text_color: 'Custom'
		text_color: app.theme_cls.text_color
		halign: 'left'
		valign: 'center'
		text: root.entry.title
