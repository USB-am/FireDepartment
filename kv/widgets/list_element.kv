<_MainScreenElementContent>:
	size_hint_y: None
	height: dp(50)
	padding: [dp(30), 0, 0, 0]

	MDBoxLayout:
		id: icons_container

	MDIconButton:
		icon: 'eye'


<IconNumberLayout>:
	size_hint_x: None
	width: self.height

	MDIcon:
		id: icon
		icon: root.icon

	MDLabel:
		id: value
		text: root.value
