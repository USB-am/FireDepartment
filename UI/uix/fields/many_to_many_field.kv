#:import os os
#:import Settings settings.settings

<ManyToManyField>:
	orientation: 'vertical'
	size_hint: 1, None
	size: self.width, 400
	padding: [10, 0]
	spacing: 10

	MDBoxLayout:
		size_hint: 1, None
		size: self.width, 50

		MDIcon:
			size_hint: None, None
			size: 50, 50
			icon: self.parent.parent.icon

		MDLabel:
			text: self.parent.parent.title

	ScrollView:
		GridLayout:
			cols: 1
			height: self.minimum_height
			size_hint: 1, None
			do_scroll_x: False
			spacing: 10
			id: many_to_many_container