
<FDCallHumanField>:
	orientation: 'horizontal'
	size_hint: (1, None)
	height: dp(50)

	FDTripleCheckbox:
		id: triple_checkbox

	MDLabel:
		halign: 'left'
		valign: 'center'
		markup: True
		text: f'{root.human.title}\n[color=#717171]{root.human.phone_1}[/color]'

	MDIconButton:
		id: info_btn
		icon: 'information'
