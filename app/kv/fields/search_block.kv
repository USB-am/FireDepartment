#:import FDTextField app.tools.custom_widgets
#:import FDIconButton app.tools.custom_widgets.button


<SearchBlock>:
	FDTextField:
		id: text_field
		hint_text: 'Search...'

	FDIconButton:
		id: search_button
		icon: 'text-search'
		theme_text_color: 'Custom'
		text_color: app.theme_cls.colors[app.theme_cls.theme_style]['Background']
		md_bg_color: app.theme_cls.primary_color