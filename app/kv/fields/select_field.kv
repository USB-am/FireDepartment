#:import FDIcon,FDLabel app.tools.custom_widgets.label


<SelectField>:
	size_hint: 1, None
	size: self.width, 50

	MDIcon:
		size_hint: None, None
		size: 50, 50
		icon: root.icon

	MDLabel:
		id: label
		text: root.display_text

	MDFloatingActionButton:
		id: open_dropmenu_button
		icon: root.icon
		md_bg_color: app.theme_cls.accent_color
		on_release:
			root.menu.open()