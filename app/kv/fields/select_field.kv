#:import FDIcon,FDLabel app.tools.custom_widgets.label


<SelectField>:
	size_hint: 1, None
	size: self.width, 50

	MDIcon:
		size_hint: None, None
		size: 50, 50
		icon: root.icon

	MDLabel:
		text: root.display_text

	MDRectangleFlatButton:
		id: open_dropmenu_button
		text: 'MDRectangleFlatButton'
		on_release:
			root.menu.open()